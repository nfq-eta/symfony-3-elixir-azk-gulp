<!DOCTYPE html>
<html {{ silvestra_seo_html_attributes() }}>
<head {{ silvestra_seo_head_attributes() }}>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1.0, user-scalable=no">

  {{ silvestra_seo_meta() }}

  {# Favicons #}
  <link rel="apple-touch-icon-precomposed" sizes="57x57" href="{{ asset('public/images/favicons/apple-touch-icon-57x57.png') }}" />
  <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset('public/images/favicons/apple-touch-icon-114x114.png') }}" />
  <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('public/images/favicons/apple-touch-icon-72x72.png') }}" />
  <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('public/images/favicons/apple-touch-icon-144x144.png') }}" />
  <link rel="apple-touch-icon-precomposed" sizes="60x60" href="{{ asset('public/images/favicons/apple-touch-icon-60x60.png') }}" />
  <link rel="apple-touch-icon-precomposed" sizes="120x120" href="{{ asset('public/images/favicons/apple-touch-icon-120x120.png') }}" />
  <link rel="apple-touch-icon-precomposed" sizes="76x76" href="{{ asset('public/images/favicons/apple-touch-icon-76x76.png') }}" />
  <link rel="apple-touch-icon-precomposed" sizes="152x152" href="{{ asset('public/images/favicons/apple-touch-icon-152x152.png') }}" />
  <link rel="icon" type="image/png" href="{{ asset('public/images/favicons/favicon-196x196.png') }}" sizes="196x196" />
  <link rel="icon" type="image/png" href="{{ asset('public/images/favicons/favicon-96x96.png') }}" sizes="96x96" />
  <link rel="icon" type="image/png" href="{{ asset('public/images/favicons/favicon-32x32.png') }}" sizes="32x32" />
  <link rel="icon" type="image/png" href="{{ asset('public/images/favicons/favicon-16x16.png') }}" sizes="16x16" />
  <link rel="icon" type="image/png" href="{{ asset('public/images/favicons/favicon-128.png') }}" sizes="128x128" />
  <link rel="icon" type="image/png" href="{{ asset('public/images/favicons/favicon.ico') }}">
  <meta name="application-name" content="&nbsp;"/>
  <meta name="msapplication-TileColor" content="#FFFFFF" />
  <meta name="msapplication-TileImage" content="{{ asset('public/images/favicons/mstile-144x144.png') }}" />
  <meta name="msapplication-square70x70logo" content="{{ asset('public/images/favicons/mstile-70x70.png') }}" />
  <meta name="msapplication-square150x150logo" content="{{ asset('public/images/favicons/mstile-150x150.png') }}" />
  <meta name="msapplication-wide310x150logo" content="{{ asset('public/images/favicons/mstile-310x150.png') }}" />
  <meta name="msapplication-square310x310logo" content="{{ asset('public/images/favicons/mstile-310x310.png') }}" />

  {% for id in google_site_verification_ids %}
    <meta name="google-site-verification" content="{{ id }}" />
  {% endfor %}

  {% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('public/styles/main.css') }}">
    <!-- build:css styles/vendor.css -->
    <!-- bower:css -->
    <!-- endbower -->
    <!-- endbuild -->

  {% endblock stylesheets %}
  <link href='https://fonts.googleapis.com/css?family=Oxygen:400,700,300&amp;subset=latin,latin-ext' rel='stylesheet' type='text/css'>
  {{ silvestra_seo_links() }}
  {{ silvestra_seo_link_canonical() }}
  {{ silvestra_seo_title() }}

  {% if app.environment != 'dev' and app.environment is not empty %}
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function()
        { (i[r].q=i[r].q||[]).push(arguments)}
        ,i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      ga('create', '{{ google_analytics_id }}', 'auto');
      ga('send', 'pageview');
    </script>
  {% endif %}
</head>
<body{% if silvestra_flash_alerts_has() %} class="has-flash"{% endif %}>

{# Mobile sidemenu #}
{% cache 'mobile_header_menu' menu_cache_lifetime %}
{{ nfq_menu_render('header_menu', app.request.locale, 'NfqOgminaBundle:Global/MobileMenu:main.html.twig') }}
{% endcache %}

{# Compare by photo #}
{# TODO: Hidden for next stage #}
{#{% include 'NfqOgminaBundle:CompareImages:index.html.twig' %}#}

{% block flash_messages %}
  {{ silvestra_flash_alerts_render({'template': 'NfqOgminaBundle:Global:flash.html.twig'}) }}
{% endblock flash_messages %}

<div class="l-site-wrap">
  <div class="l-site-wrap__overlay"></div>
  <div class="container-fluid">
    <div class="row">

      {# Header #}
      {% include 'NfqOgminaBundle:Global/Header:main.html.twig' %}

      {# Page header #}
      {% if page_title is defined %}
        {% include 'NfqOgminaBundle:Global:page-title.html.twig' %}
      {% endif %}

      {# Content #}
      {% block content %}{% endblock content %}

      {# Scroll-up button#}
      {% include 'NfqOgminaBundle:Global:scrollup.html.twig' %}

      {# Footer #}
      {% include 'NfqOgminaBundle:Global/Footer:main.html.twig' %}

      {# Cookies #}
      {% include 'NfqOgminaBundle:Global:cookies.html.twig' %}

    </div>
  </div>
</div>

{# Breakpoints (mandatory) #}
<div class="device-xs hidden-sm-up"></div>
<div class="device-sm hidden-xs-down hidden-md-up"></div>
<div class="device-md hidden-sm-down hidden-lg-up"></div>
<div class="device-lg hidden-md-down hidden-xl-up"></div>
<div class="device-xl hidden-lg-down"></div>

{% block bottom_javascripts %}
  <script src="{{ asset('public/scripts/main.js') }}"></script>

  <script src="{{ asset('public/scripts/compare-image.js') }}"></script>
  <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
  <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
  <script async src="//d1ks1friyst4m3.cloudfront.net/toolbar/prod/td.js" data-trackduck-id="{{ trackduck_id }}"></script>

  <!-- build:js scripts/vendor.js -->
  <!-- bower:js -->
  <script src="/bower_components/jquery/dist/jquery.js"></script>
  <!-- endbower -->
  <!-- endbuild -->

  <!-- build:js scripts/plugins.js -->
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/affix.js"></script>
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/alert.js"></script>
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/dropdown.js"></script>
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/tooltip.js"></script>
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/modal.js"></script>
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/transition.js"></script>
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/button.js"></script>
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/popover.js"></script>
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/carousel.js"></script>
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/scrollspy.js"></script>
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/collapse.js"></script>
  <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/tab.js"></script>
  <!-- endbuild -->

  <!-- build:js scripts/main.js -->
  <script src="/resources/assets/js/main.js"></script>
  <!-- endbuild -->
{% endblock bottom_javascripts %}
</body>
</html>
